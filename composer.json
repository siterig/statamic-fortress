{
    "name": "siterig/fortress",
    "autoload": {
        "psr-4": {
            "Siterig\\Fortress\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siterig\\Fortress\\Tests\\": "tests"
        }
    },
    "require": {
        "statamic/cms": "^5.0",
        "geoip2/geoip2": "^2.13"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Fortress",
            "description": "Secure your Statamic site from malicious attacks and malware"
        },
        "laravel": {
            "providers": [
                "Siterig\\Fortress\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
